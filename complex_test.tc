int fibonacci(int n) {
    if (n <= 1) {
        return n;
    } else {
        return fibonacci(n - 1) + fibonacci(n - 2);
    }
}

void print_numbers(int limit) {
    int i = 0;
    while (i < limit) {
        int result = fibonacci(i);
        i = i + 1;
    }
}

int main() {
    int x = 5;
    int y = 10;
    int sum = x + y;
    
    if (sum > 10) {
        print_numbers(sum);
        return sum * 2;
    } else {
        return sum;
    }
}
